		format        "%7d";
		value         "$innfeed_spooled{$key}";
		total         "total(%innfeed_spooled)";
        };
        column {
                name          "%Took";
		format_name   "%5s";
		format        "%3d%%";
		value         "$innfeed_offered{$key} == 0 ? 0 :
		       $innfeed_accepted{$key} / $innfeed_offered{$key} * 100";
		total         "total(%innfeed_offered) == 0 ? 0 :
