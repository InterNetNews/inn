		data {
			name	"Rejected";
			color	"#FFAF00";
			value	"%innfeed_rejected_size";
		};
		data {
			name	"Total";
			color	"#00FF00";
			value	"%innfeed_accepted_size +
                                 %innfeed_rejected_size";
		};
	};
};

